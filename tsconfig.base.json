{
 "compilerOptions": {
  "moduleResolution": "node",
  "removeComments": false,
  "target": "es5",
  "strict": true,
  "strictNullChecks": false,
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "preserveConstEnums": true,
  "sourceMap": true,
  "lib": [
      "es5",
      "es2015.promise",
      "dom",
      "scripthost",
      "es2015.iterable",
      "es2015.core"
    ],
  "sourceRoot": ".",
  "baseUrl": ".",
  "rootDir": ".",
  "paths": {
    "poker/*": [ "js/*" ],
    "tests/*": [ "tests/*" ]
  }
 },
 "compileOnSave": false,
 "exclude": [
     "js/messages.ts",
     "node_modules",
     "tests"
 ]
}
